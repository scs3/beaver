{
  "version": 2,
  "builds": [
    { "src": "api/beaver.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/beaver", "dest": "/api/beaver.js" },
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|gif))", "continue": true },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}